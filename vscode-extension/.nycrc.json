{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": ["text", "lcov", "html"],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test/**/*",
    "**/*.d.ts",
    "out/**/*"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}